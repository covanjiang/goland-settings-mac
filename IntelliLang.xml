<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="SQL" injector-id="go">
      <display-name>SQL alter/drop/truncate table</display-name>
      <single-file value="true" />
      <place disabled="true"><![CDATA[goStringLiteralThatMatches(" *(ALTER|DROP|TRUNCATE) +TABLE .*")]]></place>
    </injection>
    <injection language="SQL" injector-id="go">
      <display-name>SQL create table</display-name>
      <single-file value="true" />
      <place disabled="true"><![CDATA[goStringLiteralThatMatches(" *CREATE +((TEMP(ORARY)?|(GLOBAL +TEMPORARY)) +)?TABLE .*\\(.*")]]></place>
    </injection>
    <injection language="SQL" injector-id="go">
      <display-name>SQL insert</display-name>
      <single-file value="true" />
      <place disabled="true"><![CDATA[goStringLiteralThatMatches(" *INSERT +((OR +)?IGNORE +)?INTO .*")]]></place>
    </injection>
    <injection language="SQL" injector-id="go">
      <display-name>SQL select</display-name>
      <single-file value="true" />
      <place disabled="true"><![CDATA[goStringLiteralThatMatches(" *SELECT .*FROM .*")]]></place>
    </injection>
  </component>
</application>